{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    // "suppressImplicitAnyIndexErrors":true,// 取消 window['$loading'] 报错
    "noImplicitAny": false, // 取消 引入js写的类等提示报错: Could not find a declaration file for module 
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*":["src/*"]
    },
    "types": [
      "vite/client","pinia-plugin-persist"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/views/project/panel/analyseSeries/clip/components/ts/clip-plane.js"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "suppressImplicitAnyIndexErrors": true
}
